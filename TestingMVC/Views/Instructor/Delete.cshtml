@model Instructor
@{
    ViewData["Title"] = "Delete";
}

<h3>Delete Instructor</h3>
<p>Are you sure you want to delete the instructor <strong>@Model.Name</strong>?</p>

<form method="post" action="/Instructor/SaveDelete/@Model.Id">
    <button type="submit" class="btn btn-danger">Yes, Delete</button>
    <a href="/Instructor/Index" class="btn btn-secondary">Cancel</a>
</form>
